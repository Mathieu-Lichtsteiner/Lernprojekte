		<LangVersion>9.0</LangVersion>
		<UseWPF>true</UseWPF>
		<UseWindowsForms>true</UseWindowsForms>
		<IntermediateOutputPath>$(IntermediateOutputPath)\Lernprojekte\$(ProjectName)</IntermediateOutputPath>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="PropertyChanged.Fody" Version="3.3.1" PrivateAssets="All" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="Resources\welding-mask.ico">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	</ItemGroup>

</Project>
